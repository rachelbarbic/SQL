SELECT SUM (price), item_name
FROM superstore
GROUP BY item_name;

SELECT AVG (price), average_rating
FROM superstore
GROUP BY average_rating;


SELECT AVG (price), category, item_id
FROM superstore
WHERE category="Kitchen Supplies"
GROUP BY item_id;

SELECT SUM (price), category, average_rating
FROM superstore
WHERE category="Furnishings"
GROUP BY average_rating;
